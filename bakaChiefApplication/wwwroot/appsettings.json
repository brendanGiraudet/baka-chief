{
  "ASPNETCORE_ENVIRONMENT": "Production",
  "ASPNETCORE_HTTPS_PORT": "3535",
  "ASPNETCORE_Kestrel": {
    "Certificates": {
      "Default": {
        "KeyPath": "/https/private.key",
        "Path": "/https/certificate.crt"
      }
    }
  },
  "ASPNETCORE_URLS": "https://+:3535",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BakaChiefAPI": {
    "BaseUrl": "https://recip-api.brendan-giraudet.com"
  },
  "ApplicationVersion": "2.9.1",
  "SearchDelay": 1,
  "SearchConfiguration": {
    "DefaultNumberOfItemsToTake": 15,
    "DefaultNumberOfItemsToSkip": 0,
    "DefaultNumberOfItemsToTakeInForm": 3,
    "MaxNumberOfItemsToTake": 100
  }
}